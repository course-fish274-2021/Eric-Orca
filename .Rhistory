ggplot() +#for wing size
geom_histogram(data=avian, aes(x = F_wing,alpha=0.3),fill="blue")+ #construct histogram with color blue
geom_histogram(data=avian, aes(x = M_wing,alpha=0.3),fill="black")+
labs( x = "Female and Male wing size")+#change axis title
scale_x_log10()+#(scale by log)
facet_wrap(~Family)#make subplots
#exercise 4 (1)
#only includes families with more than 25 species
large_n_families <- avian %>% #copy dataset
filter(!is.na(M_mass), !is.na(F_mass)) %>%
group_by(Family)%>%
summarize(num_species = n())%>%
filter(num_species > 25)#filter families with species number less than 25
na.strings = c("-999", "-999.0","-999.00")#eliminate NA values
avian_combined<- inner_join(avian,large_n_families)#use inner_join to combine data
ggplot() +
geom_histogram(data=avian_combined, aes(x = F_mass),alpha=0.3,fill="blue")+ #construct histogram with color blue for females
geom_histogram(data=avian_combined, aes(x = M_mass),alpha=0.3,fill="yellow")+#construct histogram with color yellow for males
labs( x = "Mass(g)")+#change axis title
scale_x_log10()+#(scale by log)
facet_wrap(~Family)#make subplots
#(2)
avian_difference<- mutate(avian,difference=((F_mass - M_mass) / F_mass))#mutate and create new variable in a new dataset
ggplot()+
geom_histogram(data=avian_difference, aes(x = difference))+
xlim(-2.5,1)+#plot data and modify the limits
geom_vline(aes(xintercept = 0), colour="black")#add vertical line at 0
#(3)
avian_difference<- mutate(avian_combined,difference=((F_mass - M_mass) / F_mass))#mutate and create new variable in a new dataset
ggplot()+
geom_histogram(data=avian_difference, aes(x = difference))+
geom_vline(data=avian,aes(xintercept = 0))+#add vertical line at 0
xlim(-2.5,1)+#plot data and modify the limits
facet_wrap(~Family)#make subplots

#exercise 3
getwd()
#import data avian by import Dataset on top right, click from text(base) and find avian in data_raw folder, and select to import dataset.Make sure set tabs for separator.
na.strings = c("-999", "-999.00")#eliminate NA values
ggplot() +
geom_histogram(data=avian, aes(x = F_mass,alpha=0.3),fill="blue")+ #construct histogram with color blue
geom_histogram(data=avian, aes(x = M_mass,alpha=0.3),fill="black")+
